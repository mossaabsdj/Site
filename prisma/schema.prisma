generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id     Int     @id @default(autoincrement())
  Title  String
  Desc   String
  Prix   Float    // Changed from String to Float

}

model Admin {
  id       Int    @id @default(autoincrement())
  User     String @unique
  Password String

}
